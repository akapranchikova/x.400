{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**", "target/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**", "test-results/**"]
    },
    "coverage": {
      "dependsOn": ["test"],
      "outputs": ["coverage/**", "test-results/**"]
    },
    "gateway-tests": {
      "dependsOn": ["^build"],
      "outputs": ["test-results/**"]
    },
    "directory-tests": {
      "dependsOn": ["^build"],
      "outputs": ["test-results/**"]
    },
    "e2e": {
      "dependsOn": ["build"],
      "outputs": ["playwright-report/**", "test-results/**"],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "format": {
      "outputs": []
    },
    "docs": {
      "dependsOn": ["^docs"],
      "outputs": ["site/**"]
    }
  }
}
